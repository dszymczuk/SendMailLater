{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "script"
  },
  "rules": {
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "block-scoped-var": 1,
    "complexity": [1,20],
    "default-case": 2,
    "dot-location": [1,"property"],
    "eqeqeq": [1,"smart"],
    "guard-for-in": 1,
    "no-alert": 1,
    "no-caller": 2,
    "no-div-regex": 1,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 1,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-native-reassign": 1,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 1,
    "no-proto": 1,
    "no-redeclare": 2,
    "no-return-assign": [2,"always"],
    "no-script-url": 1,
    "no-self-compare": 2,
    "no-throw-literal": 2,
    "no-useless-call": 1,
    "no-void": 1,
    "no-warning-comments": [1,{"terms":["todo"," tofix"," fix"],"location":"start"}],
    "radix": 2,
    "vars-on-top": 1,
    "wrap-iife": [1,"any"],
    "yoda": [1,"never",{"exceptRange":true,"onlyEquality":true}],
    "init-declarations": [1,"always"],
    "no-delete-var": 1,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [2,"nofunc"],
    "brace-style": [1,"1tbs",{"allowSingleLine":true}],
    "comma-spacing": [1,{"after":true}],
    "comma-style": [1,"last"],
    "computed-property-spacing": [1,"never"],
    "max-nested-callbacks": [2,10],
    "new-parens": 2,
    "no-continue": 2,
    "no-inline-comments": 1,
    "no-multiple-empty-lines": [2,{"max":2}],
    "no-nested-ternary": 2,
    "no-new-object": 1,
    "no-spaced-func": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "object-curly-spacing": [1,"never",{}],
    "operator-linebreak": [1,"after"],
    "padded-blocks": [1,"never"],
    "quote-props": [1,"consistent-as-needed"],
    "quotes": [1,"single", { "allowTemplateLiterals": true }],
    "semi-spacing": [2,{"before":false,"after":true}],
    "semi": [2,"always"],
    "space-before-function-paren": [2,"never"],
    "space-in-parens": [2,"never"],
    "key-spacing": 2,
    "spaced-comment": [1,"always",{}],
    "wrap-regex": 1,
    "no-cond-assign": [2,"always"],
    "no-console": 1,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-parens": [2,"all"],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2,"functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "callback-return": 1,
    "handle-callback-err": 1,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-sync": 1,
    "arrow-parens": [1,"as-needed"],
    "arrow-spacing": [1,{"before":true,"after":true}],
    "no-const-assign": 2,
    "no-var": 2,
    "prefer-const": 1
  }
}